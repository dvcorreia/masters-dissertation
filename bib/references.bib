@inproceedings{ashidaNovelPreventionTechnology2014,
  title = {The Novel Prevention Technology of the Standing Wave by {{SWC}} ({{Standing Wave Canceller}}) {{SWC}} for Passive {{RFID}} Tags in All Metallic Shelf},
  booktitle = {2014 44th {{European Microwave Conference}}},
  author = {Ashida, Hiroshi and Ohashi, Yoji},
  year = {2014},
